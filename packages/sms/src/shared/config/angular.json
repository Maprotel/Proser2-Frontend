{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sms": {
      "projectType": "application",
      "schematics": {},
      "root": "packages/sms",
      "sourceRoot": "packages/sms/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./extra-webpack.config.js"
            },
            "outputPath": "dist/sms",
            "index": "packages/sms/src/index.html",
            "main": "packages/sms/src/main.ts",
            "polyfills": "packages/sms/src/polyfills.ts",
            "tsConfig": "packages/sms/tsconfig.app.json",
            "aot": false,
            "assets": [
              "packages/sms/src/favicon.ico",
              "packages/sms/src/assets"
            ],
            "styles": [
              "packages/sms/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/sms/src/environments/environment.ts",
                  "with": "packages/sms/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sms:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sms:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sms:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/sms/src/test.ts",
            "polyfills": "packages/sms/src/polyfills.ts",
            "tsConfig": "packages/sms/tsconfig.spec.json",
            "karmaConfig": "packages/sms/karma.conf.js",
            "assets": [
              "packages/sms/src/favicon.ico",
              "packages/sms/src/assets"
            ],
            "styles": [
              "packages/sms/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/sms/tsconfig.app.json",
              "packages/sms/tsconfig.spec.json",
              "packages/sms/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "packages/sms/e2e/protractor.conf.js",
            "devServerTarget": "sms:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sms:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "proser"
}