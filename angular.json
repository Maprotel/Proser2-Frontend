{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "crud": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "packages/crud",
      "sourceRoot": "packages/crud/src",
      "prefix": "app-crud",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./extra-webpack.config.js"
            },
            "outputPath": "dist/crud",
            "index": "packages/crud/src/index.html",
            "main": "packages/crud/src/main.ts",
            "polyfills": "packages/crud/src/polyfills.ts",
            "tsConfig": "packages/crud/tsconfig.app.json",
            "aot": true,
            "assets": [
              "packages/crud/src/favicon.ico",
              "packages/crud/src/shared/assets",
              {
                "glob": "**/*",
                "input": "./shared/assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "shared/styles/app.scss",
              "packages/crud/src/shared/styles/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "shared/environments/environment.ts",
                  "with": "shared/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "crud:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "crud:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "crud:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/crud/src/test.ts",
            "polyfills": "packages/crud/src/polyfills.ts",
            "tsConfig": "packages/crud/tsconfig.spec.json",
            "karmaConfig": "packages/crud/karma.conf.js",
            "assets": [
              "packages/crud/src/favicon.ico",
              "packages/crud/src/shared/assets",
              {
                "glob": "**/*",
                "input": "./shared/assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "shared/styles/app.scss",
              "packages/crud/src/shared/styles/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/crud/tsconfig.app.json",
              "packages/crud/tsconfig.spec.json",
              "packages/crud/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "packages/crud/e2e/protractor.conf.js",
            "devServerTarget": "crud:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "crud:serve:production"
            }
          }
        }
      }
    },
